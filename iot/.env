# Contoh template .env untuk project web-iot

# API Sebesi (data terbaru)
REACT_APP_API_SEBESI=https://c-greenproject.org:7070/climate_latest

# API Kalimantan (data per topik dan periode)
REACT_APP_API_KALIMANTAN_TOPIC1=https://c-greenproject.org:3000/getDataTopic1
REACT_APP_API_KALIMANTAN_TOPIC2=https://c-greenproject.org:3000/getDataTopic2
REACT_APP_API_KALIMANTAN_TABLE_TOPIC1=https://c-greenproject.org:3000/TableDataTopic1
REACT_APP_API_KALIMANTAN_TABLE_TOPIC2=https://c-greenproject.org:3000/TableDataTopic2
REACT_APP_API_KALIMANTAN_ONEDAY_TOPIC1=https://c-greenproject.org:3000/getDataForOneDayTopic1
REACT_APP_API_KALIMANTAN_ONEDAY_TOPIC2=https://c-greenproject.org:3000/getDataForOneDayTopic2
REACT_APP_API_KALIMANTAN_SEVENDAYS_TOPIC1=https://c-greenproject.org:3000/getDataForSevenDaysTopic1
REACT_APP_API_KALIMANTAN_SEVENDAYS_TOPIC2=https://c-greenproject.org:3000/getDataForSevenDaysTopic2
REACT_APP_API_KALIMANTAN_ONEMONTH_TOPIC1=https://c-greenproject.org:3000/getDataForOneMonthTopic1
REACT_APP_API_KALIMANTAN_ONEMONTH_TOPIC2=https://c-greenproject.org:3000/getDataForOneMonthTopic2

# API Petengoran (isi jika sudah ada)
REACT_APP_API_PETENGORAN=

# API key jika diperlukan
REACT_APP_API_KEY=your_api_key_here

# Google Maps API Key (jika menggunakan peta dinamis)
REACT_APP_GOOGLE_MAPS_KEY=your_google_maps_key_here

# Nama aplikasi
REACT_APP_APP_NAME=Web IoT Monitoring

# Mode environment (development/production)
REACT_APP_ENV=development

# Port (jika ingin custom, default: 3000)
PORT=3000